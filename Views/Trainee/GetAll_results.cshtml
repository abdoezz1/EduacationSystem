@model List<Trainee>

    <table class="table">
    <thead>
        <tr>
            <th scope="col">Trainee ID : </th>
            <th scope="col">Name</th>
            <th scope="col">Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var t in Model)
        {
                           <tr>
                             <th scope="row">@t.Id</th>
                             <td>@t.Name</td>
                             <td>@t.Address</td>
                             <td><a href="/Crs_result/InsertResult/@t.Id" class="btn btn-primary">Add Result</a></td>
                             <td><a href="/Trainee/Update/@t.Id" class="btn btn-primary">Update</a></td>
                             <td><a href="/Trainee/Delete/@t.Id" class="btn btn-primary">Delete</a></td>
                                


                           </tr>
        }

     </tbody>

 </table>
